{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "pnpm run build",
		"beforeDevCommand": "pnpm run dev",
		"frontendDist": "../build",
		"devUrl": "http://localhost:5173"
	},
	"bundle": {
		"active": true,
		"category": "DeveloperTool",
		"copyright": "",
		"shortDescription": "An LLM chat application",
		"externalBin": [],
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"targets": "app",
		"longDescription": "",
		"macOS": {
			"entitlements": null,
			"exceptionDomain": "",
			"frameworks": [],
			"providerShortName": null,
			"signingIdentity": null
		},
		"resources": [],
		"linux": {
			"deb": {
				"depends": []
			}
		}
	},
	"productName": "tauri-svelte",
	"version": "0.1.0",
	"identifier": "com.agavram.dev",
	"plugins": {},
	"app": {
		"macOSPrivateApi": true,
		"withGlobalTauri": true,
		"windows": [
			{
				"fullscreen": false,
				"decorations": false,
				"center": true,
				"visible": false,
				"transparent": true,
				"maximizable": false,
				"height": 700,
				"width": 600
			}
		],
		"security": {
			"csp": null
		}
	}
}
